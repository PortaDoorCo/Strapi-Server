{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "orders",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "linePrice": {
      "type": "json"
    },
    "orderType": {
      "type": "string"
    },
    "orderNum": {
      "type": "integer"
    },
    "total": {
      "type": "decimal",
      "default": 0
    },
    "subTotals": {
      "type": "json"
    },
    "Date": {
      "type": "date"
    },
    "itemPrice": {
      "type": "json"
    },
    "qty": {
      "type": "json"
    },
    "status": {
      "type": "string"
    },
    "tax": {
      "type": "float",
      "default": 0
    },
    "files": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "tracking": {
      "type": "json"
    },
    "dueDate": {
      "type": "datetime"
    },
    "late": {
      "type": "boolean"
    },
    "balance_paid": {
      "type": "decimal",
      "default": 0
    },
    "balance_due": {
      "type": "decimal",
      "default": 0
    },
    "balance_history": {
      "type": "json"
    },
    "user": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    },
    "companyprofile": {
      "autoPopulate": true,
      "via": "orders",
      "model": "companyprofiles"
    },
    "misc_items": {
      "type": "json"
    },
    "Taxable": {
      "type": "boolean",
      "default": false
    },
    "Sample": {
      "type": "boolean",
      "default": false
    },
    "Rush": {
      "type": "boolean",
      "default": false
    },
    "Conversation_Notes": {
      "type": "json"
    },
    "mouldings": {
      "type": "json"
    },
    "discount": {
      "type": "float",
      "default": 0
    },
    "sale": {
      "model": "sale",
      "via": "orders"
    },
    "part_list": {
      "type": "component",
      "repeatable": true,
      "component": "part-list.part-list"
    },
    "delivery": {
      "model": "deliveries",
      "via": "order"
    },
    "job_info": {
      "type": "component",
      "repeatable": false,
      "component": "job-info.job-info"
    },
    "scan_date": {
      "type": "datetime"
    },
    "Shipping_Scheduled": {
      "type": "boolean",
      "default": false
    },
    "DateOrdered": {
      "type": "datetime"
    },
    "DateInvoiced": {
      "type": "datetime"
    },
    "filesInfo": {
      "type": "json"
    },
    "DateShipped": {
      "type": "datetime"
    },
    "DateCompleted": {
      "type": "datetime"
    },
    "DateInProduction": {
      "type": "datetime"
    },
    "exported": {
      "type": "boolean",
      "default": false
    },
    "poNum": {
      "type": "string"
    },
    "flat_stock": {
      "type": "json"
    },
    "oldPricing": {
      "type": "boolean",
      "default": false
    },
    "cash_total": {
      "type": "decimal"
    },
    "NonCashPayment": {
      "type": "boolean"
    }
  }
}
